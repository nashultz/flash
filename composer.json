{
  "name": "nashultz/flash",
  "description": "Sweet Alert flash notifications in Laravel.",
  "keywords": ["flash", "sweet", "alert", "sweetalert", "message", "notification"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.2.5",
    "laravel/framework": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "~5.3"
  },
  "autoload": {
    "psr-4": {
      "Nashultz\\Flash\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nashultz\\Flash\\Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
